<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="pythonnet" Version="3.0.5" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="fsrs\" />
  </ItemGroup>
      <Target Name="CopyFsrs" AfterTargets="Build">
    <ItemGroup>
        <fsrs Include="fsrs\*.*" /> 
    </ItemGroup>
    <Copy SourceFiles="@(fsrs)" DestinationFolder="$(TargetDir)fsrs\" />
    </Target>
</Project>
